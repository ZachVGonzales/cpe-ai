# OpenAI Configuration (Required for RAG service)
OPENAI_API_KEY=your-api-key-here
OPENAI_MODEL=gpt-4o
PROMPT_TEMPLATE_PATH=data/system-prompts/default.jinja

# Vector Store Configuration
VECTOR_STORE_PATH=./data/vector-store/databases/
CONTEXT_NAMES=lean-api,lean-info
DOCS_PER_CONTEXT=10

# Embeddings and Reranking
EMBEDDINGS_MODEL=mixedbread-ai/mxbai-embed-large-v1
RERANKER_MODEL=mixedbread-ai/mxbai-rerank-large-v2
RERANK_BATCH_SIZE=16
HUGGINGFACE_CACHE_DIR=./cache/huggingface

# Other Settings
LOG_LEVEL=INFO
DEBUG=False